{
    "extends": [
      "airbnb",
      "prettier"
    ],
    "plugins": [
      "prettier",
      "import",
      "react"
    ],
    "parser": "babel-eslint",
    "env": {
      "es6": true,
      "browser": true
    },
    "rules": {
      "react/jsx-filename-extension": 0,
      "no-console": "off",
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "es5",
          "singleQuote": true,
          "arrowParens": "always",
          "semi": true
        }
      ]
    }
  } 